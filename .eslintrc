{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier", "react", "react-hooks"],
    "rules": {
        "react/jsx-filename-extension": "off",
        "react/react-in-jsx-scope": "off",
        "jsx-a11y/label-has-associated-control": "off",
        "react/prop-types": "off"
    }
}
